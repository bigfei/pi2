$ORIGIN bigfei.lan.
$TTL 86400 ; 1 day
@  IN SOA ns1.bigfei.lan. admin.bigfei.lan. (
    2009074711 ; serial
    7200       ; refresh (2 hours)
    300        ; retry (5 minutes)
    604800     ; expire (1 week)
    60         ; minimum (1 minute)
    )
  IN NS ns1.bigfei.lan.
  IN NS ns2.bigfei.lan.
ns1  IN A 192.168.88.168
ns2  IN A 192.168.88.54
pd-gw              A 192.168.88.254
px-gw              A 192.168.88.1
ubnt               A 192.168.88.253
esxi               A 192.168.88.166
sip                A 192.168.88.165
pi2                A 192.168.88.168

